<?xml version='1.0' encoding='UTF-8'?>
<widget xmlns= "http://www.w3.org/ns/widgets"
        xmlns:gap= "http://phonegap.com/ns/1.0"
        id= "com.fondaargentina.mobile"
        versionCode= "10" 
        version= "1.0.10" >
  
    <name>MaxxDjRadio</name>
    <description>
        Aplicaci√≥n para dispositivos moviles MaxxDjRadio.
    </description>
    <author email="rodrigo@reactivoestudio.com" href="http://www.reactivoestudio.com">
        Reactivo Estudio
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="phonegap-version" value="3.7.0" />
    <preference name="fullscreen" value="false" />
    <preference name="orientation" value="portrait" />
    <preference name="ShowTitle" value="false"/>
    
    <feature name="http://api.phonegap.com/1.0/geolocation"/>


    <!-- PLATFORM ANDROID -->
        <feature name="InAppBrowser">
            <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
        </feature>

        <feature name="Geolocation">
            <param name="android-package" value="org.apache.cordova.geolocation.GeoBroker" />
        </feature>


    <!-- PLATFORM IOS -->
        <feature name="Geolocation">
            <param name="ios-package" value="CDVLocation" />
        </feature>

        <feature name="InAppBrowser">
            <param name="ios-package" value="CDVInAppBrowser" />
        </feature>
    
</widget>